generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Tarefa {
  id          String   @id @default(uuid())
  descricao   String
  concluida   Boolean
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}