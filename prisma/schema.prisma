generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/client"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Tarefa {
  id            String    @id @db.Uuid
  descricao     String    @db.VarChar(100)
  concluida     Boolean
  criadoEm      DateTime?  @default(now())
  atualizadoEm  DateTime?  @updatedAt @default(now())
}